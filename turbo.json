{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build:watch": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": [],
      "inputs": [
        "src/**/*.ts",
        "test/**/*.ts"
      ],
      "cache": false
    },
    "lint": {
      "outputs": [],
      "inputs": [
        "src/**/*.ts"
      ],
      "cache": false
    }
  }
}
